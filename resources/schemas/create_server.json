										},
										"path": {
											"type":"string",
											"required":false
										}
									}
								}
						}
					}
				}
			}
		},
		"response": {
			"type":"object",
			"required":true,
			"properties":{
				"server": {
					"type":"object",
					"required":true,
					"properties":{
